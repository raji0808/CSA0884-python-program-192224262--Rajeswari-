def divide_numbers(num1, num2):
    try:
        result = num1 / num2
        return result
    except ZeroDivisionError:
        return "Error: Division by zero is not allowed."

# Example usage
numerator = 10
denominator = 0

result = divide_numbers(numerator, denominator)
print(result)  # Output will be "Error: Division by zero is not allowed."

# Another example
numerator = 10
denominator = 2

result = divide_numbers(numerator, denominator)
print(result)  # Output will be 5.0
